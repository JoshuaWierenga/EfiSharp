    </Compile>
    <Compile Include="..\runtimelab\src\libraries\System.Console\src\System\ConsoleModifiers.cs">
      <Link>System\ConsoleModifiers.cs</Link>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Include="System\Console.Buffer.cs" />
